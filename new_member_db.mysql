CREATE DATABASE IF NOT EXISTS new_member_db;

USE new_member_db;

CREATE TABLE user_tbl
    (firstname VARCHAR(50),
    lastname VARCHAR(50),
    email VARCHAR (50),
    password VARCHAR(255),
    address VARCHAR (50),
    city VARCHAR (50),
    state VARCHAR (50),
    zip INT (10),
    user_ID  int(6)  AUTO_INCREMENT PRIMARY KEY);

